def search(cards, query):
    # Brut force
    if len(cards)==0:
        return 'empty arr'
    else:
        for i in range(len(cards)):
            if query == cards[i]:
                return i
        return 'Item not found'

cards1 = [6, 4, -5, 3, 6]
query = 4

#print(search(cards1, query))
test={
    'input':{
        'cards':[],
        'query': 4
    },
    'output':00
}

print(search(test['input']['cards'],test['input']['query']))
